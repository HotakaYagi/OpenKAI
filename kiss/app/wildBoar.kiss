		{
			"name":"mRF",
			"kSpd":3000.0,
			"kStr":-3000.0,
			"_ActuatorBase":"motRF",
			"iAxis":0,
		}
		{
			"name":"mLR",
			"kSpd":3000.0,
			"kStr":3000.0,
			"_ActuatorBase":"motLR",
			"iAxis":0,
		}
		{
			"name":"mRR",
			"kSpd":3000.0,
			"kStr":-3000.0,
			"_ActuatorBase":"motRR",
			"iAxis":0,
		}
	}
}


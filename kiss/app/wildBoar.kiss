{
	"name":"APP",
	"class":"Startup",
	"appName":"RCrover",
	"bStdErr":1,
	"bLog":1,
}

{
	"name":"console",
	"class":"_Console",
	"bON":1,
    {
		"name":"thread",
		"FPS":30,
	}
	"vBASE":["/sSBus","sbus","RCrover","drive","motLF"],
}

{
	"name":"state",
	"class":"_StateControl",
    {
		"name":"thread",
		"FPS":30,
	}
	"bON":1,
	"start":"standby",
	{
		"name":"state",
		{
			"bON":1,
			"name":"standby",
			"class":"State",
		}
		{
			"bON":1,
			"name":"rc",
			"class":"State",
		}
	}
}

{
	"name":"sSBus",
	"class":"_SerialPort",
	"bON":1,
    {
		"name":"thread",
		"FPS":1,
	}
    {
		"name":"threadR",
		"vTwakeup":["sbus.threadR"],
	}
	"port":"/dev/ttyUSB0",
	"baud":115200,
}

{
	"name":"sbus",
	"class":"_SBus",
	"bON":1,
    {
		"name":"thread", 
		"FPS":30,
	}
    {
		"name":"threadR",
		"FPS":100,
	}
	"bLog":1,
	"bSend":0,
	"_IOBase":"sSBus",
	"vRawRC":[353,995,1637],
	"bRawSbus":1,
}

{
	"name":"RCrover",
	"class":"_RCrover",
	"bON":1,
    {
		"name":"thread",
		"FPS":30,
	}
	"bLog":0,
	"iSpd":2,
	"iSteer":0,
	"iElev":8,
	"nSpd":0.5,
	"nSteer":0.5,
	"_SBus":"sbus",
	"_Drive":"drive",
	"_ActuatorBase":"stepper",
}

{
	"name":"drive",
	"class":"_Drive",
    {
		"name":"thread",
		"FPS":30,
	}
	"bON":1,
	"nSpd":0.0,
	"nDir":1.0,
	"nStr":0.0,
	"vSpdRange":[-1.0, 1.0],
	"vStrRange":[-1.0, 1.0],
	
	{
		"name":"motor",
		
		{
			"name":"mLF",
			"kSpd":3000.0,
			"kStr":3000.0,
			"_ActuatorBase":"motLF",
			"iAxis":0,

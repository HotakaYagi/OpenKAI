{
	"name":"APP",
	"class":"Startup",
	"appName":"OpenKAI",
	"bLog":1,
	"bStdErr":1,
}

{
	"name":"OKconsole",
	"class":"_Console",
	"bON":0,
    {
		"name":"thread",
		"FPS":10,
	}
	"vBASE":["cam"],
}

{
	"name":"arduIO",
	"class":"_SerialPort",
	"bON":1,
	"bLog":0,
    {
		"name":"thread",
		"FPS":30,
	}
	"bStream":1,
	"port":"/dev/ttyUSB0",
	"baud":115200,
	"_wakeUp":"arduServo",
}

{
	"name":"arduIO",
	"class":"_UDP",
	"bON":0,
	"bLog":1,
    {
		"name":"thread",
		"FPS":30,
	}
	"addr":"192.168.1.100",
	"port":8888,
	"_wakeUp":"arduServo",
}

{
	"name":"arduServo",
	"class":"_ArduServo",
	"bON":1,
	"bLog":1,
    {
		"name":"thread",
		"FPS":30,
	}
	"_IOBase":"arduIO",
	
	"vNormTargetPos":[1.0],
	
	{
		"pwmL":1600,
		"pwmH":2200,
		"dir":-1,
	}
	{
		"pwmL":800,
		"pwmH":1500,
		"dir":1,
	}
}
